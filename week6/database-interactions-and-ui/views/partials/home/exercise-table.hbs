<table>
  <caption>Logged Workouts</caption>
  <thead>
    <tr>
      <th>Name</th>
      <th>Reps</th>
      <th>Weight</th>
      <th>Date</th>
      <th>Unit</th>
      <th>Options</th>
    </tr>
  </thead>
  <tbody id="tableBody">
    {{#each this}}
      <tr data-id={{this.id}}>
        <td>{{this.name}}</td>
        <td>{{this.reps}}</td>
        <td>{{this.weight}}</td>
        <td>{{this.date}}</td>
        <td>{{this.unit}}</td>
        <td>
          <button class="delete-workout">Delete</button>
          <button>Edit</button>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>